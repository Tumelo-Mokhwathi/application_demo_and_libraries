{
  "name": "vue_table_library",
  "version": "2.0.0",
  "description": "",
  "main": "dist/vue_table_library.umd.js",
  "unpkg": "dist/vue_table_library.umd.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "webpack-dev-server --config webpack/webpack.dev.conf.js --hot --progress -d",
    "prebuild": "rimraf ./dist",
    "build": "vue-cli-service build",
    "build:npm": "vue-cli-service build --target lib --name vue_table_library src/components/VueTable.vue",
    "build-lib": "vue-cli-service build --target lib --name vue_table_library src/components/VueTable.vue"
  },
  "dependencies": {
    "bootstrap": "^5.2.2",
    "bootstrap-vue": "^2.23.1",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.5",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-airbnb": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "chokidar": "^2.1.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.13.1",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.14",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">=12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "@vue/airbnb"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
